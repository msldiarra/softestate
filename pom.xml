    <modelVersion>4.0.0</modelVersion>

    <groupId>com.ul.realestate</groupId>
    <artifactId>softestate</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>softestate</name>
