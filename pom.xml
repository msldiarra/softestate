    <build>

        <testResources>
            <testResource>
                <directory>src/test/resources</directory>
                <filtering>true</filtering>
            </testResource>
        </testResources>

        <finalName>${project.artifactId}</finalName>
        <plugins></plugins>
        <pluginManagement>
            <plugins></plugins>
        </pluginManagement>
